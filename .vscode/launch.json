  "version": "0.2.0",
  "configurations": [
    {
      "name": "stress",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "args": [
        "-u",
        "http://google.com",
        "-c",
        "5",
        "-r",
        "10"
      ]
    },
    {
      "name": "stress2",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "args": [
        "--url",
        "http://google.com",
        "--concurrency",
        "5",
        "--requests",
        "5"
      ]
    }
  ]
